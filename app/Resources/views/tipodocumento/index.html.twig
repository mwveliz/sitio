{% extends 'base.html.twig' %}

{% block body %}
    <h1>TipoDocumento list</h1>

    <table>
        <thead>
            <tr>
                <th>Descripcion</th>
                <th>Id</th>
                <th>Actions</th>
            </tr>
        </thead>
        <tbody>
        {% for tipoDocumento in tipoDocumentos %}
            <tr>
                <td><a href="{{ path('tipodocumento_show', { 'id': tipoDocumento.id }) }}">{{ tipoDocumento.descripcion }}</a></td>
                <td>{{ tipoDocumento.id }}</td>
                <td>
                    <ul>
                        <li>
                            <a href="{{ path('tipodocumento_show', { 'id': tipoDocumento.id }) }}">show</a>
                        </li>
                        <li>
                            <a href="{{ path('tipodocumento_edit', { 'id': tipoDocumento.id }) }}">edit</a>
                        </li>
                    </ul>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('tipodocumento_new') }}">Create a new entry</a>
        </li>
    </ul>
{% endblock %}
