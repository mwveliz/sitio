<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="MAT\SitioBundle\Entity\Menu" table="menu">
    <id name="id" type="bigint" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="descripcion" type="string" column="descripcion" nullable="true"/>
    <field name="posicion" type="string" column="posicion" nullable="true"/>
    <field name="visible" type="boolean" column="visible" nullable="true"/>
    <field name="enlace" type="string" column="enlace" nullable="true"/>
  </entity>
</doctrine-mapping>
