<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="MAT\SitioBundle\Entity\ImagenNoticiainterna" table="imagen_noticiainterna">
    <indexes>
      <index name="IDX_F1C78C0DA52E675E" columns="id_imagen"/>
      <index name="IDX_F1C78C0DE258A667" columns="id_noticiainterna"/>
    </indexes>
    <id name="id" type="bigint" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <many-to-one field="idImagen" target-entity="Imagen">
      <join-columns>
        <join-column name="id_imagen" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idNoticiainterna" target-entity="NoticiaInterna">
      <join-columns>
        <join-column name="id_noticiainterna" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
